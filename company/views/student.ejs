<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List Student</title>
</head>
<body>
    <h1>List Student</h1>
    <a href="/students/add">Add Student</a>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Phase</th>
                <th>Like Mcd</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach (student => { %> 
                <tr>
                    <td><%= student.id %> </td>
                    <td><%= student.name %> </td>
                    <td><%= student.phase %> </td>
                    <td><%= student.likeMcd %> </td>
                    <td><a href="/students/edit/<%= student.id %>">Edit</a> | <a href="/students/delete/<%= student.id %>">Delete</a></td>
                </tr>
            <% }) %> 
        </tbody>
    </table>
</body>
</html>